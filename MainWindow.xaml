<Window x:Class="Bajnding2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bajnding2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<StackPanel>
		
		<Button Content="Unos" FontSize="42" Click="Unos" />
		<Label Content="Osoba: " FontSize="42"/>
		<Label Content="{Binding Ime}" FontSize="42"/>
		<Label Content="{Binding Prezime}" FontSize="42"/>
		<Label Content="{Binding Godine}" FontSize="42"/>
	</StackPanel>
</Window>
